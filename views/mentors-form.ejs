    <!-- Registration Form Design -->
    <section id="form">
        <h3 id="form-heading">Registration for mentors </h3>
        <p>Mentors will offer
            assistance to the contributors. They will be available to contribute to Github,assist
            contributors in creating content, answer questions, manage the subject repository, and
            provide generally assistance to contributors throughout the winter.</p>
        <form action="/mentorSuccess" method="post">
            <div>
                <label for="name">Name</label><br>
                <input type="text" name="name" id="name" required>
            </div>

            <div>
                <label for="email">Email</label><br>
                <input type="email" name="email" id="email" required>
            </div>

            <div>
                <label for="contact">Phone Number</label><br>
                <input type="tel" name="contact" id="contact" required>
            </div>

            <div>
                <label for="college-name">College/School Name</label><br>
                <input type="text" name="college" id="college-name" required>
            </div>

            <div>
                <label for="year">Year</label><br>
                <select name="year" id="year">
                    <option value="default" selected disabled>Choose your current year</option>
                    <option value="first">First</option>
                    <option value="second">Second</option>
                    <option value="third">Third</option>
                    <option value="fourth">Fourth</option>
                </select>
            </div>

            <div>
                <label for="stream">Stream</label><br>
                <input type="text" name="stream" id="stream">
            </div>

            <div class="domain-options">
                <label for="domain">Your preferred domain</label>
                <div class="left-options">
                    <div>
                        <input type="checkbox" id="android-flutter" name="domain" value="Android Development with Flutter">
                        <label for="android-flutter">Android Development with Flutter</label>
                    </div>
                    <div>
                        <input type="checkbox" id="android-java" name="domain" value="Android Development with Java">
                        <label for="android-java">Android Development with Java</label>
                    </div>
                    <div>
                        <input type="checkbox" id="android-kotlin" name="domain" value="Android Development with Kotlin">
                        <label for="android-kotlin">Android Development with Kotlin</label>
                    </div>
                    <div>
                        <input type="checkbox" id="backend-php" name="domain" value="Backend Development with PHP">
                        <label for="backend-php">Backend Development with PHP</label>
                    </div>
                    <div>
                        <input type="checkbox" id="c/c++" name="domain" value="C/C++">
                        <label for="c/c++">C/C++</label>
                    </div>
                    <div>
                        <input type="checkbox" id="comp-coding" name="domain" value="Competitive Coding">
                        <label for="comp-coding">Competitive Coding</label>
                    </div>
                    <div>
                        <input type="checkbox" id="cyber-security" name="domain" value="Cyber Security">
                        <label for="cyber-security">Cyber Security</label>
                    </div>
                    <div>
                        <input type="checkbox" id="dsa" name="domain" value="Data Structures and Algorithm">
                        <label for="dsa">Data Structures and Algorithm</label>
                    </div>
                    <div>
                        <input type="checkbox" id="dsr" name="domain" value="Data Science with R">
                        <label for="dsr">Data Science with R</label>
                    </div>
                    <div>
                        <input type="checkbox" id="dsp" name="domain" value="Data Science with Python">
                        <label for="dsp">Data Science with Python</label>
                    </div>
                </div>

                <div class="right-options">
                    <div>
                        <input type="checkbox" id="golang" name="domain" value="Golang">
                        <label for="golang">Golang</label>
                    </div>
                    <div>
                        <input type="checkbox" id="interview-prep" name="domain" value="Interview Preparation">
                        <label for="interview-prep">Interview Preparation</label>
                    </div>
                    <div>
                        <input type="checkbox" id="java" name="domain" value="Java">
                        <label for="java">Java</label>
                    </div>
                    <div>
                        <input type="checkbox" id="javascript" name="domain" value="JavaScript">
                        <label for="javascript">JavaScript</label>
                    </div>
                    <div>
                        <input type="checkbox" id="mern" name="domain" value="MERN">
                        <label for="mern">MERN</label>
                    </div>
                    <div>
                        <input type="checkbox" id="open-source" name="domain" value="Open Source">
                        <label for="open-source">Open-Source</label>
                    </div>
                    <div>
                        <input type="checkbox" id="rust" name="domain" value="Rust">
                        <label for="rust">Rust</label>
                    </div>
                    <div>
                        <input type="checkbox" id="stats" name="domain" value="Statistics">
                        <label for="stats">Statistics</label>
                    </div>
                    <div>
                        <input type="checkbox" id="ui-ux" name="domain" value="UI/UX">
                        <label for="ui-ux">UI/UX</label>
                    </div>
                </div>
            </div>

            <div class="open-source-knowledge">
                <label for="open-source-knowledge">Do you have any previous experience in open-source?</label>
                <div>
                    <input type="radio" id="yes" name="openSource" value="yes">
                    <label for="openSource">Yes</label>
                </div>
                <div>
                    <input type="radio" id="no" name="openSource" value="no">
                    <label for="openSource">No</label>
                </div>
            </div>

            <div>
                <label for="whyContribute">How will GWOC be beneficial for you?</label>
                <textarea name="whyContribute" id="whyContribute"></textarea>
            </div>

            <button type="submit" value="submit" id="register-contributor">SUBMIT</button>
        </form>
    </section>

    
